{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "plugins": [
    "react"
  ],  
  "rules": {
 "semi":"off",
 "comma-spacing":"off",
 "no-console":"off",
 "quotes":"off",
 "no-unused-vars":"warn",
 "comma-dangle":"off",
 "no-confusing-arrow":"off"
    
  }
}
